
# name of image to be removed
IMGNAME=rdimg

# name of container to be removed
CONTNAME=rdcont

# name of volume used
VOLNAME=app-volume

id=$(docker ps -aqf "name=$CONTNAME")
docker stop $id
docker rm $id
docker rmi $(docker images $IMGNAME -a -q)
docker system prune -f

docker volume rm -f $VOLNAME